generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Pet {
  id    Int    @id @default(autoincrement())
  name  String
  owner String
}

model Post {
  id Int @id @default(autoincrement())
  title String
  content String
}
